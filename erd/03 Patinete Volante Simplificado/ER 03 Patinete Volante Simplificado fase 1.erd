erdiagram Model
notation=crowsfoot

entity Patinete {
    matricula key
    marca
    modelo
    fecha_compra
    fecha_ultima_revision
    precio_minuto
    estado
}

entity Zona {
    id_zona key
    nombre
    habitantes
    max_patinetes
}

relationship asignado_a {
    Patinete[1..N] -> Zona[1..1]
}   

relationship circula_por {
    Patinete[1..N] -> Zona[1..N]
}   


