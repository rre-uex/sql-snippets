erdiagram Model
notation=crowsfoot

entity Superheroe {
    nombreSuperheroe key  
    fecha
}

entity Planeta {
    cod_planeta key  
    nombrePlaneta
    sistema_planetario
    coordenadas
}

relationship procede {
    Superheroe[0..N] -> Planeta[0..1]
}

entity Superpoder {
    cod_superpoder key  
    descripcion
}

relationship posee {
    Superheroe[0..N] -> Superpoder[1..N]  
    nivel
}




